
public class MainApplication {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		M m = new M();
		m.generate(10);
      

	}

}

class M {
	public void generate(int num ) {
		  for(int i = 1; i <= 10; ++i)
	        {
	            System.out.printf("%d * %d = %d \n", num, i, num * i);
	        }
	}
}